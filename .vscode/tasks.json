{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Run Scheduler",
            "type": "shell",
            "command": "/Users/jasons/Source/github/forest_backend/forest_backend_env/bin/python",
            "args": [
                "${workspaceFolder}/forest_backend/scheduler/scheduler.py"
            ]
        },
        {
            "label": "Run postgres",
            "type": "shell",
            "command": "pg_ctl",
            "args": [
                "-D", "/usr/local/var/postgres",
                "-l", "logfile",
                "restart"
            ]
        },
        {
            "label": "Stop postgres",
            "type": "shell",
            "command": "pg_ctl",
            "args": [
                "-D", "/usr/local/var/postgres",
                "stop"
            ]
        }
    ]
}